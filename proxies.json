{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "File Proxy": {
            "matchCondition": {
                "route": "/{*file}",
                "methods": [
                    "GET"
                ]
            },
            "backendUri": "https://localhost/api/getfile?file={file}"
        }
    }
}