{
    "proxies": {
        "chipps.create": {
            "matchCondition": {
                "methods": [ "POST" ],
                "route": "/api/chipps/{table}/{partition}"
            },
            "backendUri": "%chipps_api%/api/chipps/{table}/{partition}"
        },        
        "chipps.read": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/api/chipps/{table}/{partition}/{id}"
            },
            "backendUri": "%chipps_api%/api/chipps/{table}/{partition}/{id}"
        },
        "chipps.readall": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/api/chipps/{table}/{partition}"
            },
            "backendUri": "%chipps_api%/api/chipps/{table}/{partition}"
        },
        "chipps.update": {
            "matchCondition": {
                "methods": [ "PUT" ],
                "route": "/api/chipps/{table}/{partition}/{id}"
            },
            "backendUri": "%chipps_api%/api/chipps/{table}/{partition}/{id}"
        },
        "chipps.delete": {
            "matchCondition": {
                "methods": [ "DELETE"],
                "route": "/api/chipps/{table}/{partition}/{id}"
            },
            "backendUri": "%chipps_api%/api/chipps/{table}/{partition}/{id}"
        },
        "file.get": {
            "matchCondition": {
                "route": "/{*file}",
                "methods": [ "GET" ]
            },
            "backendUri": "%file_api%/api/getfile?file={file}"
        }
    }
}