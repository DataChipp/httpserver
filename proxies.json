{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "API Proxy": {
            "matchCondition": {
                "route": "/api/getrequest"
            },
            "backendUri": "https://localhost/api/getrequest"
        },
        "File Proxy": {
            "matchCondition": {
                "route": "/{*file}",
                "methods": [
                    "GET"
                ]
            },
            "backendUri": "%WEB_HOST%/api/getfile?file={file}"
        }
    }
}
